{
  "name": "@loupeteam/atn",
  "version": "0.5.1",
  "description": "Loupe's atn library for Automation Runtime",
  "homepage": "https://loupeteam.github.io/Sandbox/libraries/atn.html",
  "scripts": {},
  "keywords": [],
  "author": "Loupe",
  "license": "Loupe Software License Agreement",
  "repository": {
    "type": "git",
    "url": "https://github.com/loupeteam/atn"
  },
  "lpm": {
    "type": "library",
    "physical": {
      "cpu": [
        {
          "config": "intel",
          "attributes": {
            "AdditionalLibraryDirectories": "../LoupeRT/lib/",
            "AdditionalLibraries": "AR",
            "Debugging": "true"
          }
        },
        {
          "config": "arm",
          "attributes": {
            "AdditionalLibraryDirectories": "../LoupeRT/lib/Arm",
            "AdditionalLibraries": "AR",
            "Debugging": "true"
          }
        }
      ]
    }
  },
  "dependencies": {
    "@loupeteam/stringext": "*",
    "@loupeteam/vartools": "*",
    "@loupeteam/tlsf": "*"
  }
}